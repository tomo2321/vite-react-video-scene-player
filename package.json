{
  "name": "vite-react-video-scene-player",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "description": "Interactive video player with synchronized subtitles and advanced language learning features",
  "keywords": [
    "video-player",
    "subtitles",
    "language-learning",
    "react",
    "typescript",
    "srt",
    "vtt"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lint": "biome check --write ./src && tsc -b && vite build",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "test": "echo \"No tests specified\" && exit 0",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "husky": "^9.1.7",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
